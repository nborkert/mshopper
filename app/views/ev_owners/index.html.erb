<h1>Listing ev_owners</h1>

<table>
  <tr>
    <th>Email address</th>
    <th>Lat</th>
    <th>Lon</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ev_owners.each do |ev_owner| %>
  <tr>
    <td><%= ev_owner.email_address %></td>
    <td><%= ev_owner.lat %></td>
    <td><%= ev_owner.lon %></td>
    <td><%= link_to 'Show', ev_owner %></td>
    <td><%= link_to 'Edit', edit_ev_owner_path(ev_owner) %></td>
    <td><%= link_to 'Destroy', ev_owner, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

